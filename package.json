{
  "name": "@plaited/behavioral",
  "version": "2.0.2",
  "engines": {
    "node": ">= 14.2.0",
    "npm": ">= 6.14"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint . --quiet --fix --ext .ts",
    "lint:ci": "eslint .  --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "npm run build",
    "prepare": "husky install",
    "start": "snowpack dev",
    "test": "ava",
    "test:ci": "c8 --check-coverage --statements 90 --branches 90 --functions 90 --lines 90 --include src/**/*.ts --exclude src/index.ts npm test",
    "test:watch": "npm test -- --watch --verbose"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run test:ci && format-package -w"
    }
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/mocha": "^8.2.2",
    "@types/snowpack-env": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@web/dev-server-esbuild": "^0.2.12",
    "@web/test-runner": "^0.13.5",
    "autoprefixer": "^10.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "0.12.6",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-mocha": "^9.0.0",
    "format-package": "^6.1.0",
    "husky": "^6.0.0",
    "ini": ">=2.0.0",
    "snowpack": "^3.5.5",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "y18n": ">=5.0.8"
  }
}